{
  "name": "portfolio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.10.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts --max-old-space-size=4096 start",
    "build": "GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:prod": "npm run build && npm run analyze-bundle",
    "analyze-bundle": "source-map-explorer 'build/static/js/*.js' --html bundle-report.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild": "npm run clean",
    "clean": "rimraf build",
    "optimize-assets": "node scripts/optimize-assets.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "craco": "^7.1.0",
    "@craco/craco": "^7.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "terser-webpack-plugin": "^5.3.10"
  }
}
